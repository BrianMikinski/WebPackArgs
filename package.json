{
  "name": "webpackargs",
  "version": "1.0.0",
  "description": "How to pass command line values to webpack",
  "main": "index.js",
  "private": true,
  "scripts": {
    "build": "webpack --config=webpack.config.js --progress --profile --color --env build=dev",
    "build:prod": "webpack --config=webpack.config.js --progress --profile --color --env build=prod",
    "serve": "webpack serve --open --config=webpack.config.js --progress --profile --color --env build=dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BrianMikinski/WebPackArgs.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/BrianMikinski/WebPackArgs/issues"
  },
  "homepage": "https://github.com/BrianMikinski/WebPackArgs#readme",
  "devDependencies": {
    "webpack": "^5.38.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "lodash": "^4.17.21",
    "terser-webpack-plugin": "^5.1.3"
  }
}
